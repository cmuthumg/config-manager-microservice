package com.config.db.serivce.repository;

import java.io.Serializable;

import org.springframework.data.repository.CrudRepository;
import org.springframework.stereotype.Repository;

import com.config.db.serivce.model.Online;

@Repository
public interface OnlineRepository extends CrudRepository<Online, Serializable> {

	Online findByServiceContextNameAndStatus(String serviceContextName, String status);

	Online findByServiceContextName(String serviceContextName);

	Long deleteByServiceContextNameAndStatus(String serviceContextName, String status);

	Long deleteByServiceContextName(String serviceContextName);
}
