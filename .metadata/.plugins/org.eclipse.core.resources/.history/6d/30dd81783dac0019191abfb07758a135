package com.config.db.serivce.repository;

import org.springframework.data.cassandra.repository.CassandraRepository;

import com.config.db.serivce.model.Online;

public interface OnlineRepository extends CassandraRepository<Online> {

	Online findByServiceContextNameAndStatus(String serviceContextName, String status);

	Online findByServiceContextName(String serviceContextName);
}
