package com.config.db.serivce.registry;

import java.util.List;

public interface AdapterService<T, U> {
	
	List<U> getAllService();
	
	public U getConfigData(String id, String status);

	public U saveconfigData(T data);

	public U updateConfigData(T data);

	public void deleteConfigData(String id, String status);

}
