package com.config.db.serivce.controller;

import javax.websocket.server.PathParam;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import com.config.db.serivce.registry.ServiceRegistry;

@RestController
@RequestMapping("/dbserivce")
public class DBServiceController {

	@Autowired
	private ServiceRegistry serviceRegistry;

	@GetMapping("/service/{servicename}/{resourcename}/{status}")
	public String getConfigData(@PathParam("servicename") String servicename,
			@PathParam("resourcename") String resourcename, @PathParam("status") String status) {

		return "";
	}

}
