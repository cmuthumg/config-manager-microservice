package com.config.db.serivce.repository;

import java.io.Serializable;

import org.springframework.data.repository.CrudRepository;

import com.config.db.serivce.model.Offline;

public interface OfflineRepository extends CrudRepository<Offline, Serializable> {
	Offline findByServiceContextNameAndStatus(String serviceContextName, String status);

	Offline findByServiceContextName(String serviceContextName);
	
	Long deleteByServiceContextNameAndStatus(String serviceContextName, String status);
	
	Long deleteByServiceContextName(String serviceContextName);
}
