package com.config.db.serivce.model;

import org.springframework.cassandra.core.PrimaryKeyType;
import org.springframework.data.cassandra.mapping.PrimaryKey;
import org.springframework.data.cassandra.mapping.PrimaryKeyColumn;
import org.springframework.data.cassandra.mapping.Table;

@Table("ONLINE_CONFIG")
public class Online {
	@PrimaryKey("Service_Context_Name")
	private String serviceContextName;

	@PrimaryKeyColumn(name = "STATUS", ordinal = 2, type = PrimaryKeyType.CLUSTERED)
	private String status;

	public Online() {
		// Do nothing
	}

	public Online(String serviceContextName, String status) {
		super();
		this.serviceContextName = serviceContextName;
		this.status = status;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getServiceContextName() {
		return serviceContextName;
	}

	public void setServiceContextName(String serviceContextName) {
		this.serviceContextName = serviceContextName;
	}
}
